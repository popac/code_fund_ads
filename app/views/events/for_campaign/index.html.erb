<%= render(PageComponent.new(subject: @eventable)) do |component| %>
  <%= component.with(:header) { render(Page::HeaderComponent.new(title: "Events", subtitle: "List of events related to this campaign")) } %>
  <% component.with(:body) do %>
    <%= render CardComponent.new do %>
      <%= render("/events/table", eventable: @eventable, events: @events) %>
    <% end %>
  <% end %>
<% end %>
